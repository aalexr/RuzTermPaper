<Page
    x:Class="RuzTermPaper.Pages.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:appmodel="using:Windows.ApplicationModel"
    mc:Ignorable="d">

    <Pivot x:Name="NavView" Canvas.ZIndex="0" IsTabStop="False" SelectionChanged="NavView_SelectionChanged_1">
        <Pivot.Items>
            <PivotItem Header="View" x:Uid="MainPage_NavigationViewItem_Current" Tag="RuzTermPaper.Pages.TimetablePage">
                <Frame x:Name="PivotItem1"/>
            </PivotItem>
            <PivotItem Header="Find" x:Uid="MainPage_NavigationViewItem_Find" Tag="RuzTermPaper.Pages.FindPage">
                <Frame x:Name="PivotItem2"/>
            </PivotItem>
        </Pivot.Items>
    </Pivot>
</Page>
